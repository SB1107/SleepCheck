<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_061629">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/result_total_apnea_title_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:autoSizeMaxTextSize="30sp"
        android:text="총 이상호흡 횟수"
        android:textColor="@color/color_78899F"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/result_total_apnea_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:text="5회"
        android:textColor="@color/color_FFFFFF"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/result_apnea10_title_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"
        android:text="10초이상 이상호흡 횟수"
        android:textColor="@color/color_78899F"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/result_total_apnea_text_view" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/result_apnea10_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="30dp"
        android:text="5회"
        android:textColor="@color/color_FFFFFF"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/result_total_apnea_text_view" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/result_apnea30_title_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"
        android:text="30초이상 이상호흡 횟수"
        android:textColor="@color/color_78899F"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/result_apnea10_text_view" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/result_apnea30_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="30dp"
        android:text="5회"
        android:textColor="@color/color_FFFFFF"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/result_apnea10_text_view" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/result_apnea60_title_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"
        android:text="60초이상 이상호흡 횟수"
        android:textColor="@color/color_78899F"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/result_apnea30_text_view" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/result_apnea60_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="30dp"
        android:text="5회"
        android:textColor="@color/color_FFFFFF"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/result_apnea30_text_view" />

    <View
        android:id="@+id/v_3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="30dp"
        android:background="@color/color_1F3046"
        app:layout_constraintTop_toBottomOf="@id/result_apnea60_text_view" />

</androidx.constraintlayout.widget.ConstraintLayout>